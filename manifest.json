{
    "name": "QuickiePedia",
    "description": "Runs a QuickiePedia session on your browser",
    "version": "0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon.png",
        "32": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "background",
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}